{
  "name": "react-bookshelf",
  "version": "1.0.0",
  "description": "Mobile responsive application developed with Node/Express/React/Redux-Thunk, following the Udemy tutorial from CodeRevolution",
  "main": "/server/server.js",
  "scripts": {
    "start": "node server/server.js",
    "mongodb": "mongod",
    "server": "nodemon server/server.js",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"npm run mongodb\" \"npm run server\" \"npm run client\""
  },
  "engines": {
    "//": "this is needed for deployment with Heroku",
    "node": "8.6.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/silvestrevivo/react-bookshelf.git"
  },
  "keywords": [
    "node",
    "express",
    "react",
    "redux",
    "mongodb",
    "mongoose"
  ],
  "author": "@silvestrevivo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/silvestrevivo/react-bookshelf/issues"
  },
  "homepage": "https://github.com/silvestrevivo/react-bookshelf#readme",
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.18.3",
    "concurrently": "^4.0.1",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "jsonwebtoken": "^8.3.0",
    "moment-js": "^1.1.15",
    "mongoose": "^5.2.13"
  },
  "devDependencies": {
    "http-proxy-middleware": "^0.19.0"
  }
}
